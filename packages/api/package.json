    "type-graphql": "^0.17.5",
    "typeorm": "^0.2.20",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.12.9",
    "@nestjs/schematics": "^6.9.1",
    "@nestjs/testing": "^6.11.4",
    "@types/dotenv": "^8.2.0",
